generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Submission {
  id         String   @id @default(uuid())
  questionId String
  answer     String
  email      String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
